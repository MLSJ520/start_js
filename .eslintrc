{
  "parser": "babel-eslint",
  
"env": {
    "browser": true,
    "node": true
,"es6":true  },
  
"extends": "airbnb-base",
  
"rules": {
    
"semi": [2, "never"],
    
"arrow-body-style": ["error", "always"],
    
"operator-assignment": [0, "never"],
    
"no-console": 0,

"no-unused-vars":0,    
"no-return-assign": 0,
"no-param-reassign":
1
}
}